            <AnchorPane prefHeight="599.0" prefWidth="246.0" style="-fx-background-color: black;">
               <children>
                  <Pane layoutX="1.0" prefHeight="56.0" prefWidth="246.0" style="-fx-background-color: black;">
                     <children>
                        <Label fx:id="actualités" layoutX="57.0" layoutY="10.0" text="Actualités" textFill="#f2ecec">
