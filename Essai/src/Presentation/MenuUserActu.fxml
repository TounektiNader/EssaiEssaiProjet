            </AnchorPane>
         </children></AnchorPane>
      <AnchorPane layoutX="240.0" layoutY="4.0" prefHeight="592.0" prefWidth="585.0">
         <children>
            <ImageView fx:id="img" fitHeight="592.0" fitWidth="577.0" layoutX="8.0" pickOnBounds="true">
