<AnchorPane id="AnchorPane" prefHeight="599.0" prefWidth="792.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Presentation.MenuUserActuController">
